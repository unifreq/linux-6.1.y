/dts-v1/;
#include <dt-bindings/input/input.h>
#include "rk3328.dtsi"

/ {
	model = "Chainedbox L1 Pro";
	compatible = "pine64,rock64", "rockchip,rk3328";

	aliases {
		mmc0 = &emmc;
		mmc1 = &sdmmc;
                mmc2 = &sdio;
	};

	chosen {
		bootargs = "earlycon=uart8250,mmio32,0xff130000";
		stdout-path = "serial2:1500000n8";
	};

	leds {
		compatible = "gpio-leds";

                red-led {
                        gpios = <&gpio0 RK_PA0 GPIO_ACTIVE_HIGH>;
                        default-state = "on";
                };

                blue-led {
                        gpios = <&gpio2 RK_PC5 GPIO_ACTIVE_HIGH>;
                        default-state = "on";
                        linux,default-trigger = "stmmac-0:00:link";
                };
	};

	gmac_clkin: external-gmac-clock {
		compatible = "fixed-clock";
		clock-frequency = <125000000>;
		clock-output-names = "gmac_clkin";
		#clock-cells = <0>;
	};

        vcc_sys_12v: vcc-sys-12v {
                compatible = "regulator-fixed";
                regulator-name = "vcc_sys_12v";
                regulator-always-on;
                regulator-boot-on;
                regulator-min-microvolt = <12000000>;
                regulator-max-microvolt = <12000000>;
        };

        vcc_sys_5v: vcc-sys-5v {
                compatible = "regulator-fixed";
                regulator-name = "vcc_sys_5v";
                regulator-always-on;
                regulator-boot-on;
                regulator-min-microvolt = <5000000>;
                regulator-max-microvolt = <5000000>;
                vin-supply = <&vcc_sys_12v>;
        };

        vcc_host_5v: vcc-host-5v-regulator {
                compatible = "regulator-fixed";
                regulator-name = "vcc_host_5v";
                regulator-always-on;
                regulator-boot-on;
                regulator-min-microvolt = <5000000>;
                regulator-max-microvolt = <5000000>;
                vin-supply = <&vcc_sys_5v>;
        };

        vcc_host1_5v: vcc_otg_5v: vcc-host1-5v-regulator {
                compatible = "regulator-fixed";
                regulator-name = "vcc_host1_5v";
                regulator-always-on;
                regulator-boot-on;
                regulator-min-microvolt = <5000000>;
                regulator-max-microvolt = <5000000>;
                vin-supply = <&vcc_sys_5v>;
        };

        vcc_phy: vcc-phy-regulator {
                compatible = "regulator-fixed";
                regulator-name = "vcc_phy";
                regulator-always-on;
                regulator-boot-on;
                regulator-min-microvolt = <1050000>;
                regulator-max-microvolt = <1050000>;
        };

        vcc_fan_5v: vcc-fan-5v-regulator {
                compatible = "regulator-fixed";
                gpio = <&gpio2 RK_PB7 GPIO_ACTIVE_HIGH>;

                pinctrl-names = "default";
                pinctrl-0 = <&fan_pwr_pin>;
                regulator-name = "vcc_fan_5v";

                enable-active-high;
                regulator-boot-on;
                regulator-always-on;

                regulator-min-microvolt = <5000000>;
                regulator-max-microvolt = <5000000>;
                vin-supply = <&vcc_sys_5v>;
	};

        vcc_hdd_12v: vcc-hdd-12v-regulator {
                compatible = "regulator-fixed";
                gpio = <&gpio2 RK_PC2 GPIO_ACTIVE_HIGH>;

                pinctrl-names = "default";
                pinctrl-0 = <&hdd_pwr_pin>;
                regulator-name = "vcc_hdd_12v";

                enable-active-high;
                regulator-boot-on;
                regulator-always-on;

                regulator-min-microvolt = <12000000>;
                regulator-max-microvolt = <12000000>;
                startup-delay-us = <2000000>;
                vin-supply = <&vcc_sys_12v>;
        };

        vcc_hdd_5v: vcc-hdd-5v-regulator {
                compatible = "regulator-fixed";
                regulator-name = "vcc_hdd_5v";
                regulator-boot-on;
                regulator-always-on;
                regulator-min-microvolt = <5000000>;
                regulator-max-microvolt = <5000000>;
                vin-supply = <&vcc_sys_5v>;
        };

        vcc_hdd_3v3: vcc-hdd-3v3-regulator {
                compatible = "regulator-fixed";
                regulator-name = "vcc_hdd_3v3";
                regulator-always-on;
                regulator-boot-on;
                regulator-min-microvolt = <3300000>;
                regulator-max-microvolt = <3300000>;
                vin-supply = <&vcc_sys_5v>;
        };

        keys: gpio-keys {
                compatible = "gpio-keys";
                #address-cells = <1>;
                #size-cells = <0>;
                autorepeat;

                pinctrl-names = "default";
                pinctrl-0 = <&pwr_key>;

                button@0 {
                        wakeup-source;
                        gpios = <&gpio0 RK_PA2 GPIO_ACTIVE_LOW>;
                        linux,code = <KEY_POWER>;
                        label = "GPIO Power key";
                        linux,input-type = <1>;
                        gpio-key,wakeup = <1>;
                        debounce-interval = <100>;
                };
        };

        fan0: pwm-fan {
                compatible = "pwm-fan";
                pwms = <&pwm2 0 50000 0>;
                #cooling-cells = <2>;
		cooling-min-state = <0>;
		cooling-max-state = <3>;
                cooling-levels = <100 150 200 255>;
                fan-supply = <&vcc_sys_5v>;
                status = "okay";
        };
};

&cpu0 {
	cpu-supply = <&vdd_arm>;
};

&cpu1 {
	cpu-supply = <&vdd_arm>;
};

&cpu2 {
	cpu-supply = <&vdd_arm>;
};

&cpu3 {
	cpu-supply = <&vdd_arm>;
};

&emmc {
	max-frequency = <150000000>;
	bus-width = <8>;
	cap-mmc-highspeed;
	mmc-hs200-1_8v;
	non-removable;
	num-slots = <1>;
	pinctrl-names = "default";
	pinctrl-0 = <&emmc_clk &emmc_cmd &emmc_bus8>;
	vmmc-supply = <&vcc_io>;
	vqmmc-supply = <&vcc18_emmc>;
	status = "okay";
};

&gmac2io {
	assigned-clocks = <&cru SCLK_MAC2IO>, <&cru SCLK_MAC2IO_EXT>;
	assigned-clock-parents = <&gmac_clkin>, <&gmac_clkin>;
	clock_in_out = "input";
	phy-supply = <&vcc_phy>;
	phy-mode = "rgmii";
	pinctrl-names = "default";
	pinctrl-0 = <&rgmiim1_pins>;
	snps,aal;
	snps,pbl = <0x8>;
	snps,reset-gpio = <&gpio1 RK_PC2 GPIO_ACTIVE_LOW>;
	snps,reset-active-low;
	snps,reset-delays-us = <0 20000 100000>;
	tx_delay = <0x26>;
	rx_delay = <0x11>;
	status = "okay";
};

&gpu {
	mali-supply = <&vdd_logic>;
};

&rng {
	status = "okay";
};

&crypto {
	status = "disabled";
};

&hdmi {
	status = "okay";
};

&hdmiphy {
	status = "okay";
};

&hdmi_sound {
        status = "okay";
};

&i2c1 {
	status = "okay";

	rk805: rk805@18 {
		compatible = "rockchip,rk805";
		reg = <0x18>;
		interrupt-parent = <&gpio1>;
		interrupts = <24 IRQ_TYPE_LEVEL_LOW>;
		#clock-cells = <1>;
		clock-output-names = "xin32k", "rk805-clkout2";
		gpio-controller;
		#gpio-cells = <2>;
		pinctrl-names = "default";
		pinctrl-0 = <&pmic_int_l>;
		rockchip,system-power-controller;
		wakeup-source;

		vcc1-supply = <&vcc_sys_5v>;
		vcc2-supply = <&vcc_sys_5v>;
		vcc3-supply = <&vcc_sys_5v>;
		vcc4-supply = <&vcc_sys_5v>;
		vcc5-supply = <&vcc_io>;
		vcc6-supply = <&vcc_sys_5v>;

		regulators {
			vdd_logic: DCDC_REG1 {
				regulator-name = "vdd_logic";
				regulator-min-microvolt = <712500>;
				regulator-max-microvolt = <1450000>;
				regulator-ramp-delay = <12500>;
				regulator-always-on;
				regulator-boot-on;
				regulator-state-mem {
					regulator-on-in-suspend;
					regulator-suspend-microvolt = <1000000>;
				};
			};

			vdd_arm: DCDC_REG2 {
				regulator-name = "vdd_arm";
				regulator-min-microvolt = <712500>;
				regulator-max-microvolt = <1450000>;
				regulator-ramp-delay = <12500>;
				regulator-always-on;
				regulator-boot-on;
				regulator-state-mem {
					regulator-on-in-suspend;
					regulator-suspend-microvolt = <950000>;
				};
			};

			vcc_ddr: DCDC_REG3 {
				regulator-name = "vcc_ddr";
				regulator-always-on;
				regulator-boot-on;
				regulator-state-mem {
					regulator-on-in-suspend;
				};
			};

			vcc_io: DCDC_REG4 {
				regulator-name = "vcc_io";
				regulator-min-microvolt = <3300000>;
				regulator-max-microvolt = <3300000>;
				regulator-always-on;
				regulator-boot-on;
				regulator-state-mem {
					regulator-on-in-suspend;
					regulator-suspend-microvolt = <3300000>;
				};
			};

			vcc_18: LDO_REG1 {
				regulator-name = "vcc_18";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;
				regulator-always-on;
				regulator-boot-on;
				regulator-state-mem {
					regulator-on-in-suspend;
					regulator-suspend-microvolt = <1800000>;
				};
			};

			vcc18_emmc: LDO_REG2 {
				regulator-name = "vcc18_emmc";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;
				regulator-always-on;
				regulator-boot-on;
				regulator-state-mem {
					regulator-on-in-suspend;
					regulator-suspend-microvolt = <1800000>;
				};
			};

			vdd_10: LDO_REG3 {
				regulator-name = "vdd_10";
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <1000000>;
				regulator-always-on;
				regulator-boot-on;
				regulator-state-mem {
					regulator-on-in-suspend;
					regulator-suspend-microvolt = <1000000>;
				};
			};
		};
	};
};

&i2s0 {
	status = "okay";
};

&pwm2 {
        status = "okay";
};

&io_domains {
	status = "okay";

	vccio1-supply = <&vcc_io>;
	vccio2-supply = <&vcc18_emmc>;
	vccio3-supply = <&vcc_io>;
	vccio4-supply = <&vcc_18>;
	vccio5-supply = <&vcc_io>;
	vccio6-supply = <&vcc_io>;
	pmuio-supply = <&vcc_io>;
};

&pinctrl {

	pmic {
		pmic_int_l: pmic-int-l {
			rockchip,pins = <1 RK_PD0 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

        keys {
                pwr_key: pwr-key {
                        rockchip,pins = <0 RK_PA2 RK_FUNC_GPIO &pcfg_pull_none>;
                };
        };

        hdd {
                hdd_pwr_pin: hdd-pwr-pin {
                        rockchip,pins = <2 RK_PC2 RK_FUNC_GPIO &pcfg_pull_up>;
                };
        };

        fan {
                fan_pwr_pin: fan-pwr-pin {
                        rockchip,pins = <2 RK_PB7 RK_FUNC_GPIO &pcfg_pull_up_4ma>;
                };
        };
};

&sdmmc {
	bus-width = <4>;
	cap-mmc-highspeed;
	cap-sd-highspeed;
	cd-gpios = <&gpio1 RK_PA5 GPIO_ACTIVE_LOW>;
	disable-wp;
	pinctrl-names = "default";
	pinctrl-0 = <&sdmmc0_clk &sdmmc0_cmd &sdmmc0_dectn &sdmmc0_bus4>;
	vmmc-supply = <&vcc_io>;
	status = "disabled";
};

&tsadc {
	rockchip,hw-tshut-mode = <0>;
	rockchip,hw-tshut-polarity = <0>;
	status = "okay";
};

&uart2 {
	status = "okay";
};

&u2phy {
	status = "okay";
};

&u2phy_host {
	status = "okay";
};

&u2phy_otg {
	status = "okay";
};

&usb20_otg {
	dr_mode = "host";
	status = "okay";
};

&usb_host0_ehci {
	status = "okay";
};

&usb_host0_ohci {
	status = "okay";
};

&usbdrd3 {
        dr_mode = "host";
        status = "okay";
};

&vop {
	status = "okay";
};

&vop_mmu {
	status = "okay";
};

&soc_thermal {
	/delete-node/ trips;
	/delete-node/ cooling-maps;
        trips {
                soc_cool: soc-cool {
                        temperature = <50000>;
                        hysteresis = <2000>;
                        type = "active";
                };

                soc_warm: soc-warm {
                        temperature = <60000>;
                        hysteresis = <2000>;
                        type = "active";
                };

		threshold: trip-point0 {
			temperature = <70000>;
			hysteresis = <2000>;
			type = "active";
		};

		target: trip-point1 {
			temperature = <85000>;
			hysteresis = <2000>;
			type = "active";
		};

		soc_crit: soc-crit {
			temperature = <95000>;
			hysteresis = <2000>;
			type = "critical";
		};
        };

        cooling-maps {

                map0 {
                        trip = <&soc_cool>;
                        cooling-device = <&fan0 0 1>;
                };

                map1 {
                        trip = <&soc_warm>;
                        cooling-device = <&fan0 1 2>;
                };

                map2 {
                        trip = <&threshold>;
                        cooling-device = <&fan0 2 3>;
		};

		map3 {
			trip = <&target>;
			cooling-device = <&cpu0 THERMAL_NO_LIMIT THERMAL_NO_LIMIT>,
					 <&cpu1 THERMAL_NO_LIMIT THERMAL_NO_LIMIT>,
					 <&cpu2 THERMAL_NO_LIMIT THERMAL_NO_LIMIT>,
					 <&cpu3 THERMAL_NO_LIMIT THERMAL_NO_LIMIT>;
			contribution = <4096>;
		};
        };
};

&grf_gpio {
        status = "okay";
};

&cpu0_opp_table {

	opp-408000000 {
		status = "disabled";
	};

	opp-1392000000 {
		opp-hz = /bits/ 64 <1392000000>;
		opp-microvolt = <1400000>;
		clock-latency-ns = <40000>;
		status = "disabled";
	};

	opp-1512000000 {
		opp-hz = /bits/ 64 <1512000000>;
		opp-microvolt = <1450000>;
		clock-latency-ns = <40000>;
		status = "disabled";
	};

};
